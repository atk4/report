{
    "name": "atk4/report",
    "type": "library",
    "description": "Agile Data - Report Extension",
    "keywords": ["agile", "data", "framework", "report", "group", "aggregation", "crud"],
    "homepage": "http://agiletoolkit.org/data/extension/report",
    "license": "Commercial",
    "authors": [
        {
            "name": "Romans Malinovskis",
            "email": "romans@agiletoolkit.org",
            "homepage": "https://nearly.guru/"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2.0",
        "atk4/data": "dev-develop",
        "atk4/schema": "dev-develop"
    },
    "require-dev": {
        "phpunit/phpunit": "<6",
        "phpunit/dbunit": ">=1.2",
        "phpunit/phpcov": "*",
        "codeclimate/php-test-reporter": "*"
    },
    "autoload": {
        "psr-4": {"atk4\\report\\":"src/"}
    },
    "autoload-dev": {
        "psr-4": {
          "atk4\\report\\tests\\":"tests/"
        }
    }
}
